<script>

    window.onload = function() {
        document.querySelector('#preloader').style.display = "none";
    };

    function toggleToBasic() {
        document.querySelector('#tab1').classList.add("active");
        document.querySelector('#tab1').classList.add("show");
        document.querySelector('#tab2').classList.remove("active");
        document.querySelector('#tab2').classList.remove("show");
        document.querySelector('#link1').classList.add("active");
        document.querySelector('#link2').classList.remove("active");
    };
    function toggleToAdvanced() {
        document.querySelector('#tab2').classList.add("active");
        document.querySelector('#tab2').classList.add("show");
        document.querySelector('#tab1').classList.remove("active");
        document.querySelector('#tab1').classList.remove("show");
        document.querySelector('#link2').classList.add("active");
        document.querySelector('#link1').classList.remove("active");
    };
</script>
